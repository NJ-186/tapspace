{
  "name": "taaspace",
  "version": "4.0.0-alpha.1",
  "description": "A zoomable user interface lib for HTML elements.",
  "keywords": [
    "zoomable",
    "user interface",
    "html",
    "layout"
  ],
  "homepage": "https://github.com/taataa/taaspace",
  "main": "index.js",
  "author": {
    "name": "Akseli Palen",
    "email": "akseli.palen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/taataa/taaspace.git"
  },
  "license": "MIT",
  "dependencies": {
    "component-emitter": "^1.2.1",
    "loadimages": "^1.0.0",
    "move-js": "^0.5.0",
    "nudged": "^1.2.0",
    "seqid": "^1.0.1"
  },
  "devDependencies": {
    "async": "^2.6.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "genversion": "^1.1.3",
    "jquery": "^3.2.1",
    "semver": "^5.4.1",
    "standard": "*",
    "style-loader": "^0.19.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-run": "^3.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.9.0",
    "webpack-livereload-plugin": "^1.0.0",
    "webpack-tape-run": "0.0.7"
  },
  "scripts": {
    "start": "python3 -m http.server 8000",
    "serve": "ifconfig | grep inet && python3 -m http.server 8000",
    "build": "npm run lint && npm run gv && npm run test:headless && npm run build:min",
    "build:min": "webpack",
    "test:headless": "webpack --config test/webpack.taperun.config.js",
    "test:watch": "webpack --watch --config test/webpack.browser.config.js",
    "lint": "standard",
    "lintfix": "standard --fix",
    "gv": "genversion lib/version.js",
    "build:examples-play": "npm run test:examples-play && browserify examples/play/src/index.js -o examples/play/app.js",
    "build:examples-canvas": "npm run test:examples-canvas && browserify examples/canvas/src/index.js -o examples/canvas/app.js"
  }
}
